{
  "name": "eth-sanfrancisco",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**/*"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "test": "run-p test:*",
    "test:market-contracts": "yarn workspace market-contracts test",
    "dev": "run-p build:*",
    "dev:market-app": "yarn workspace market-contracts dev",
    "build": "run-p build:*",
    "build:market-contracts": "yarn workspace market-contracts build",
    "lint": "run-p lint:*",
    "lint:market-app": "yarn workspace market-app lint",
    "lint:market-contracts": "yarn workspace market-contracts lint",
    "prettier": "run-p prettier:*",
    "prettier:market-app": "yarn workspace market-app prettier",
    "prettier:market-contracts": "yarn workspace market-contracts prettier"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.0",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{md,json}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@worldcoin/world-id-contracts": "^0.1.1"
  }
}
