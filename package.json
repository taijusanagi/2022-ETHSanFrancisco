{
  "name": "eth-sanfrancisco",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**/*"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "patch-package",
    "test": "run-p test:*",
    "test:market-contracts": "yarn workspace market-contracts test",
    "dev": "run-p dev:*",
    "dev:app-integration": "yarn workspace app-integration dev",
    "build": "run-p build:*",
    "build:app-integration": "yarn workspace app-integration build",
    "build:market-contracts": "yarn workspace market-contracts build",
    "lint": "run-p lint:*",
    "lint:app-integration": "yarn workspace app-integration lint",
    "lint:market-contracts": "yarn workspace market-contracts lint",
    "prettier": "run-p prettier:*",
    "prettier:app-integration": "yarn workspace app-integration prettier",
    "prettier:market-contracts": "yarn workspace market-contracts prettier"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.0",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{md,json}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@worldcoin/world-id-contracts": "^0.1.1"
  },
  "resolutions": {
    "@types/react": "17.0.39"
  }
}
